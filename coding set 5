#question 16 - prime numbers
def is_prime(number):
  #prime numbers are grerater than 1
if number <= 1:
  return flase

#then check for factors from 2 to sqrt(number)
for i in range(2, int(number**0.5) + 1):
  if number % i == 0:
    return False

return True

def main():
  #next prompt the user to enter a number 
number = int(input("Enter a number:"))

#Check if the number is a prime number using is_prime function 
if is_prime(number):
    print(f{number} is a prime number.)
else: 
  print(f"{number} is not a prime number.")

if__name__=='__main__":


#the function is_prime(number)is the function that checks the number is less than or equal to 0 and if it is false it is because it is bigger than 1 
# the main() function prompts the user to input and convert the input into an integer using the function

#Prime number lists

def is_prime(number):
  if number<= 1:
    return Flase
for i in range(2, int(number**0.5) +1):
  if number % i == 0:
      return Flase
return True

def display_primes():
print("Prime number from 1 to 100:")
  from num in range(1,101):
    if if_prime(num):
      print(num, end=" ")

def main():
  display_primes()

if __name__ == "__main__":
main()

