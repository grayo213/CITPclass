# 7 stadium seating 
def main():
#constat for ticket prices 
price_class_a = 20 
price_class_b = 15
price_class_c = 10

#grab the number of tickets sold for each class

tickets_class_a = int(input("Enter the number of Class A tickets sold:"))
tickets_class_b = int(input("Enter the number of Class b tickets sold:"))
tickets_class_c = int(input("Enter the number of Class c tickets sold:"))

# calulate the totalt income generated from the tickets 

income_class_a = tickets_class * price_classs_a
income_class_b = tickets_class * price_class_b
income_class_c = tickets_class * price_class_c
total_income = income_class_a + income_class_b + income_class_c
#display the results 
print("/nIncome Report:")
print(f"Class A Tickets Sold: {tickets_class_a}")
print(f"Income from Class A: ${income_class_a}")
print(f"CLass B tickets Sold: {tickets_class_b}")
print(f"Income from Class B:${income_class_b}")
print(f"Class C Tickets Sold: {tickets_class_c}")
print(f"Income from Class C" ${income_class_c}")
print(f"Total Income: ${total_income}")

if __name__ == "__main__":
main()


